{
  "WebConfiguration": {
    "BaseUrl": "https://eshop-demo-766125429055.europe-central2.run.app",
    "UserName": "",
    "UserPassword": ""
  },
  "TestConfiguration": {
    "LogsPath": "../../../Logs",
    "StoreLogsAlways": true,
    "PageElementTimeout": 60,
    "PageLoadTimeout": 90,
    "SeleniumHubUrl": "http://localhost:4444/wd/hub",
    "IsRemote": false,
    "IsHeadless": false,
    "WindowSize": "1920,1080"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo:NUnit": {
      "Name": "NUnitOutput",
      "Args": {
        "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] [{TestMethod}] {Message:lj}{NewLine}{Exception}",
        "restrictedToMinimumLevel": "Information"
      }
    },
    "WriteTo:TestProjectLogger": {
      "Name": "Logger",
      "Args": {
        "configureLogger": {
          "WriteTo": [
            {
              "Name": "File",
              "Args": {
                "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] [{Caller}] {Message:lj}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    },
    "WriteTo:TestClassLogger": {
      "Name": "Logger",
      "Args": {
        "configureLogger": {
          "WriteTo": [
            {
              "Name": "File",
              "Args": {
                "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] [{Caller}] {Message:lj}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    },
    "WriteTo:TestMethodLogger": {
      "Name": "Logger",
      "Args": {
        "configureLogger": {
          "WriteTo": [
            {
              "Name": "File",
              "Args": {
                "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] {Message:lj}{NewLine}{Exception}",
                "restrictedToMinimumLevel": "Warning"
              }
            },
            {
              "Name": "File",
              "Args": {
                "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] {Message:lj}{NewLine}{Exception}",
                "restrictedToMinimumLevel": "Information"
              }
            },
            {
              "Name": "File",
              "Args": {
                "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] [{Caller}] {Message:lj}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    }
  }
}